    "#Send String\n",
    "import socket \n",
    "soc = socket.socket()\n",
    "hostname=\"localhost\"# 127.0.0.1 #0.0.0.0\n",
    "port=65436\n",
    "soc.bind((hostname,port))\n",
    "soc.listen(5)\n",
    "conn , addr = soc.accept()\n",
    "print(\"device connected\"+str(addr))\n",
    "# # Send String\n",
    "msg = b\"Hello From Python Server\\n\"\n",
    "conn.send(msg)\n",
    "\n",
    "soc.close()"
   ]
